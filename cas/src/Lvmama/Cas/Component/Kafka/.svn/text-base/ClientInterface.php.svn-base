<?php

namespace Lvmama\Cas\Component\Kafka;

/**
 * Interface ClientInterface
 *
 * @author libiying
 * @package Lvmama\Cas\Component\Kafka
 */
interface ClientInterface{

    /**
     * 业务逻辑处理
     *
     * @param $data
     * @return mixed
     */
    public function handle($data);

    /**
     * 错误
     * @return mixed
     */
    public function error();

    /**
     * 链接超时
     * @return mixed
     */
    public function timeOut();

}