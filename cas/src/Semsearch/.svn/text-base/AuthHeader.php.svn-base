namespace Semsearch;


class AuthHeader extends CommonType
{
    public $username;
    public $password;
    public $target;
    public $token;
    public $action = "API-SDK";
    public $accessToken;
    public $account_type;
//    public $serveToken;
//    public $apiKey;
//    public $apiSecret;
//    public $sessionToken;

    public function __construct($params = null)
    {
        $this->mapping($params);
    }

    /**
     * @return unknown
     */
    public function getAction()
    {
        return $this->action;
    }

    /**
     * @param unknown_type $action
     */
    public function setAction($action)
    {
        $this->action = $action;
    }

    /**
     * @return unknown
     */
    public function getAccessToken()
    {
        return $this->accessToken;
    }
    /**
     * @param unknown_type $accessToken
     */
    public function setAccessToken($accessToken)
    {
        $this->accessToken = $accessToken;
    }
    /**
     * @return unknown
     */
    public function getAccount_type()
    {
        return $this->account_type;
    }
    /**
     * @param unknown_type $account_type
     */
    public function setAccount_type($account_type)
    {
        $this->account_type = $account_type;
    }
    /**
     * @return unknown
     */
    public function getPassword()
    {
        return $this->password;
    }

    /**
     * @param unknown_type $password
     */
    public function setPassword($password)
    {
        $this->password = $password;
    }
    /**
     * @return unknown
     */
    public function getTarget()
    {
        return $this->target;
    }

    /**
     * @param unknown_type $target
     */
    public function setTarget($target)
    {
        $this->target = $target;
    }
    /**
     * @return unknown
     */
    public function getToken()
    {
        return $this->token;
    }

    /**
     * @param unknown_type $token
     */
    public function setToken($token)
    {
        $this->token = $token;
    }
    /**
     * @return unknown
     */
    public function getUsername()
    {
        return $this->username;
    }

    /**
     * @param unknown_type $username
     */
    public function setUsername($username)
    {
        $this->username = $username;
    }

    /**
     * @return unknown
     */
    public function getServeToken()
    {
        return $this->serveToken;
    }

    /**
     * @param unknown_type $serveToken
     */
    public function setServeToken($serveToken)
    {
        $this->serveToken = '';
    }

    /**
     * @return unknown
     */
    public function getApiKey()
    {
        return $this->apiKey;
    }

    /**
     * @param unknown_type $serveToken
     */
    public function setApiKey($apiKey)
    {
        $this->apiKey = $apiKey;
    }
    /**
     * @return unknown
     */
    public function getApiSecret()
    {
        return $this->apiSecret;
    }

    /**
     * @param unknown_type $serveToken
     */
    public function setApiSecret($apiSecret)
    {
        $this->apiSecret = $apiSecret;
    }
}